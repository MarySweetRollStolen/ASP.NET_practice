@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
<!DOCTYPE HTML>

<html>
<head>
    @await Html.PartialAsync("MetatagPartial")
    @await Html.PartiaAsync("CssPartial")
</head>
<body>
    <div id="page-wrapper">

        <!-- Header -->
        @await Html.PartialAsync("HeaderPartial")

        <!-- Main -->
        <div id="main">
            <div class="container">
                <div class="row main-row">
                    <div class="col-4 col-12-medium">
                        @*RenderBody()*@
                    </div>
                </div>
                @await Html.PartialAsync("SidebarPartial")
            </div>
        </div>

        <!-- Footer -->
        @await Html.PartialAsync("FooterPartial")

    </div>

    <!-- Scripts -->
    @await Html.PartialAsync("ScriptPartial")

</body>
</html>